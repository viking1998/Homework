package org.elsys.programming;

public class HashMapImpl {
	
	private int values[];
	
	public HashMapImpl() {
		values = new int[17];
	}

	public void add(String key, int value) {
		// TODO Auto-generated method stub
		int keyHash = key.hashCode();
		int i = keyHash % 17;
		values[i] = value;
	}
	
	public int get(String key) {
		return values[key.hashCode()%17];
	}

}
