package org.elsys.threads;

public class Philosophers extends Thread {
	
	private final int philosopherId;
	
	public Philosophers(int id, Fork left, Fork right) {
		philosopherId = id;
	}
	
	@Override
	public String toString() {
		return "Philosopher " + philosopherId;
	}
	
	@Override
	public void run() {
		
	}
}
